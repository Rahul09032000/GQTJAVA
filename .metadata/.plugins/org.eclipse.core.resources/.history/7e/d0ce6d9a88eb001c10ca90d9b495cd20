import java.util.ArrayList;

public class ArrayList2 {
	public static void main(String[] args) {
		ArrayList al = new ArrayList();
		al.add(10);
		al.add(20);
		al.add(30);
		al.add("Sachin");
		al.add('f');
		al.add(100.7f);
		al.add(233448.853);
		al.add(40);
		al.add(50);
		System.out.println(al);
		al.clear();
		System.out.println(al);
	}
}

